{
  "debug": true,
  "url": "https://bot.incolumitas.com/",
  "wait": 5,
  "driver": "selenium",
  "actions": [
    {
      "type": "input",
      "xpath": "//*[@id='userNameField']/div/input",
      "value": "not_bot_uname",
